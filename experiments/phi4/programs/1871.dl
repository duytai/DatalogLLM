.decl person(p: symbol)
.decl water_body(w: symbol)
.decl smokes(p: symbol, w: symbol)
.decl violation(p: symbol, w: symbol)

violation(p, w) :- person(p), water_body(w), smokes(p, w).
person("fisherman").
water_body("river").
smokes("fisherman", "river").
violation("fisherman", "river").
.output violation